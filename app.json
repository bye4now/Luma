{
  "expo": {
    "name": "LUMA",
    "slug": "luma",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/LUMA_App_Icon_512px.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/LUMA_App_Icon_512px.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B9FBD"
    },

    "android": {
      "package": "com.bye4now.luma",
      "versionCode": 10,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LUMA_App_Icon_512px.png",
        "backgroundColor": "#0B9FBD"
      },
      "permissions": ["RECORD_AUDIO", "VIBRATE"]
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.voice-journal-horoscope",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },

    "web": {
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      [
        "expo-router",
        { "origin": "https://rork.com/" }
      ],
      [
        "expo-av",
        { "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone" }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "eas": {
        "projectId": "3eb72231-3bcb-41d1-a50c-5fc398fba44e"
      }
    },

    "owner": "bye4now"
  }
}
